prototype(Neos.RedirectHandler.Ui:Module.Export) < prototype(Neos.Fusion:Component) {
    indexAction = Neos.Fusion:UriBuilder {
        action = 'index'
    }
    exportCsvAction = Neos.Fusion:UriBuilder {
        action = 'exportCsv'
    }

    renderer = afx`
        <div class="neos-content neos-container-fluid">
            <div class="neos-row-fluid">
                <a class="neos-button neos-button-primary" href={props.exportCsvAction}>
                    {I18n.translate('Neos.RedirectHandler.Ui:Modules:exportCsv')}
                </a>
            </div>

            <div class="neos-footer">
                <a class="neos-button" href={props.indexAction}>
                    {I18n.translate('Neos.RedirectHandler.Ui:Modules:backToList')}
                </a>
            </div>
        </div>
    `
}
