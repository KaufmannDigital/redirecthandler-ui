prototype(Neos.RedirectHandler.Ui:Module.ImportCsv) < prototype(Neos.Fusion:Component) {
    protocol = ${[]}

    indexAction = Neos.Fusion:UriBuilder {
        action = 'index'
    }

    renderer = afx`
        <div class="neos-content neos-container-fluid">
            <div class="neos-row-fluid" @if.protocolNotEmpty={props.protocol}>
                <h2>{I18n.translate('Neos.RedirectHandler.Ui:Modules:header.importProtocol')}</h2>
                <br/>
                <ul class="redirects-protocol-list">
                    <Neos.Fusion:Loop items={props.protocol} itemName="entry">
                        <li class={'redirects-protocol-entry--' + entry.type}>
                            <i class="fas fa-exclamation-triangle" @if.type={entry.type == 'error'}></i>
                            <i class="fas fa-trash" @if.type={entry.type == 'deleted'}></i>
                            <i class="fas fa-plus-circle" @if.type={entry.type == 'created'}></i>
                            <i class="fas fa-equals" @if.type={entry.type == 'unchanged'}></i>
                            &nbsp;<strong>{String.firstLetterToUpperCase(entry.type)}:</strong> {entry.message}
                            <span @if.hasRedirect={entry.redirect}>
                                {entry.redirect.sourceUriPath} &rarr; {entry.redirect.targetUriPath} ({entry.redirect.statusCode})
                            </span>
                        </li>
                    </Neos.Fusion:Loop>
                </ul>
            </div>

            <div class="neos-footer">
                <a class="neos-button" href={props.indexAction}>
                    {I18n.translate('Neos.RedirectHandler.Ui:Modules:backToList')}
                </a>
            </div>
        </div>
    `
}
